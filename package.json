{
  "name": "kgustave.me",
  "version": "1.0.0",
  "description": "Website for Kaidan Gustave.",
  "main": "index.js",
  "author": "Kaidan Gustave",
  "license": "Apache-2.0",
  "homepage": "http://shengaero.github.io/home",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-js",

    "start-css": "node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p start-css start-js",

    "test": "jest",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/react-fontawesome": "0.0.20",
    "bootstrap": "^4.1.2",
    "highlight.js": "^9.12.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.3",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-helmet": "^5.2.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^6.3.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.1.0",
    "@fortawesome/free-brands-svg-icons": "^5.1.0",
    "@fortawesome/free-regular-svg-icons": "^5.1.0",
    "@fortawesome/free-solid-svg-icons": "^5.1.0",
    "gh-pages": "^1.2.0",
    "jest": "^22.4.3",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "react-scripts": "^1.1.4",
    "react-test-renderer": "^16.4.1"
  }
}
