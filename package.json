    "start": "npm-run-all -p start-css start-ts start-js",
    "test": "jest",
    "eject": "react-scripts eject",
    "organize-deployment": "npm-run-all build gulp",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
