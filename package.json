  "license": "Apache-2.0",
  "homepage": "http://shengaero.github.io/",
  "private": true,
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "build-ts": "tsc",
    "build-js": "react-scripts build",
    "build": "npm-run-all build-css build-ts build-js",
    "start-css": "node-sass-chokidar src/ -o src/ -w",
